@{
    ViewBag.Title = "库存管理";
    Layout = "~/Views/Shared/_LayoutSysManager.cshtml";
}

<!DOCTYPE html>
<html lang="zh-Hant-TW">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>库存管理后台</title>
    <link rel="stylesheet" href="~/Content/Dashboradasset/fonts/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/Dashboradasset/css/all.css">
</head>
<body>

    <div class="pricing-header px-3 py-3 pt-md-5 pb-md-4 mx-auto text-center">
        <h1 class="display-4">库存管理</h1>
        <p class="lead">便捷入口，点击进入管理页面</p>
    </div>

    <div class="container">
        <div class="card-deck mb-3 text-center">
            <div class="card mb-4 shadow-sm">
                <div class="card-header">
                    <h4 class="my-0 font-weight-normal">纸品管理</h4>
                </div>
                <div class="card-body">
                    <h1 class="card-title pricing-card-title">管理库存纸品</h1>
                    <ul class="list-unstyled mt-3 mb-4">
                        <li>方便</li>
                        <li>快捷</li>
                        <li>高效</li>
                        <li>智能化</li>
                    </ul>
                    <a class="btn btn-lg btn-block btn-outline-primary" href="@Url.Action("PaperStockManagement","Manager")">进入</a>
                </div>
            </div>
            <div class="card mb-4 shadow-sm">
                <div class="card-header">
                    <h4 class="my-0 font-weight-normal">产品管理</h4>
                </div>
                <div class="card-body">
                    <h1 class="card-title pricing-card-title">管理产品库存</h1>
                    <ul class="list-unstyled mt-3 mb-4">
                        <li>方便</li>
                        <li>快捷</li>
                        <li>高效</li>
                        <li>智能化</li>
                    </ul>
                    <a class="btn btn-lg btn-block btn-outline-primary" href="@Url.Action("ProductStockManagement","Manager")">进入</a>
                </div>
            </div>
            <div class="card mb-4 shadow-sm">
                <div class="card-header">
                    <h4 class="my-0 font-weight-normal">油墨管理</h4>
                </div>
                <div class="card-body">
                    <h1 class="card-title pricing-card-title">管理油墨库存</h1>
                    <ul class="list-unstyled mt-3 mb-4">
                        <li>方便</li>
                        <li>快捷</li>
                        <li>高效</li>
                        <li>智能化</li>
                    </ul>
                    <a class="btn btn-lg btn-block btn-outline-primary" href="@Url.Action("PrintingInkStockManagement","Manager")">进入</a>
                </div>
            </div>
        </div>
    </div>


</body>
</html>